{
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "family": "LearnJenkinsApps-task-def",
    "containerDefinitions": [
        {
            "name": "learnjenkinsapp-prod",
            "image": "257394477950.dkr.ecr.us-east-1.amazonaws.com/jenkinsapp:1.0.77",
            "portMappings": [
                {
                    "name": "nginx-80-tip",
                    "containerPort": 80,
                    "hostPort": 80,
                    "protocol": "tcp",
                    "appProtocol": "http"
                }
            ],
            "essential": true
        }
    ],
    "volumes": [],
    "networkMode": "awsvpc",
    "memory": "512",
    "cpu": "256",
    "executionRoleArn": "arn:aws:iam::257394477950:role/ecsTaskExecutionRole"
}
